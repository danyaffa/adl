env
# API Configuration
REACT_APP_API_URL=https://api.adl.com
REACT_APP_API_VERSION=v1

# Social Media Integration
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_TIKTOK_CLIENT_KEY=your_tiktok_client_key

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Features
REACT_APP_AI_ENABLED=true
REACT_APP_LEGAL_PACKAGE_ENABLED=true
REACT_APP_CREDIT_SYSTEM_ENABLED=true

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_DATA=true

# Deployment
REACT_APP_DEPLOYMENT_URL=https://adl.vercel.app

